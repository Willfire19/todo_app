<div class="row">
	<div class="col-xs-6">
		<h3><%= list.name %></h3>
	</div>
	<div class="col-xs-3">
		<%= link_to 'EDIT', edit_user_list_path(@user, list), :class=>"btn btn-primary btn-block" %>
	</div>
	<div class="col-xs-3">
		<%= link_to "DELETE", user_list_path(@user, list), :class=>"btn btn-danger btn-block", method: :delete, data: { confirm: "You sure?" }, title: list.name %>
	</div>
</div>

<hr>